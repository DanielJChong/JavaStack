package com.daniel.zookeeper;

public class Gorilla extends Mammal {
	
	public Gorilla() {
		//super(energyLevel);
	}
	public void throwSomething() {
		System.out.println("Gorilla threw something");
		energyLevel -= 5;
	}
	public void eatBanana() {
		System.out.println("Gorilla ate something, feeling content");
		energyLevel += 10;
	}
	public void climb() {
		System.out.println("Gorilla climbed a tree");
		energyLevel -= 10;
	}
}